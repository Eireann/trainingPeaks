define(
[
],
function ()
{
    var AppliedPlanModel = TP.APIModel.extend(
    {
        cacheable: true,

        webAPIModelName: "AppliedPlan",
        idAttribute: "appliedPlanId",

        urlRoot: function()
        {
            var athleteId = theMarsApp.user.getCurrentAthleteId();
            return theMarsApp.apiRoot + "/fitness/v1/athletes/" + athleteId + "/workouts";
        },

        defaults:
        {
            "appliedPlanId": null,
        }
    });

    return AppliedPlanModel;
});
